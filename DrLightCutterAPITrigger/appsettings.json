{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndPoints": {
      "Https": {
        "Url": "https://*:5200",
        "Certificate": {
          "Path": "/var/www/Portal/ssl/fullchain.pem",
          "KeyPath": "/var/www/Portal/ssl/privkey.pem"
        }
      }
    }
  }
}
